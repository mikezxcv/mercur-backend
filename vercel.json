{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "framework": null,
  "outputDirectory": ".medusa",
  "functions": {
    "api/**/*.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/.medusa/client/$1"
    },
    {
      "src": "/app",
      "dest": "/.medusa/client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "rewrites": [
    {
      "source": "/app/(.*)",
      "destination": "/.medusa/client/$1"
    },
    {
      "source": "/app",
      "destination": "/.medusa/client/index.html"
    }
  ]
}